{
  "name": "vue3-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build:prod": "vue-tsc && vite build --mode production",
    "build-only:prod": "vite build --mode test",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"src/**/*.{html,vue,ts,js,cjs,json,md}\"",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue,html}\" --fix",
    "prepare": "husky install",
    "zip:prod": "vue-tsc && vite build --mode production && python plugins/zip.py",
    "font-subset": "python plugins/font-subset.py"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@faker-js/faker": "9.8.0",
    "@vueuse/core": "^13.3.0",
    "axios": "1.10.0",
    "crypto-js": "^4.2.0",
    "element-plus": "^2.10.2",
    "gm-crypto": "^0.1.12",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "sm-crypto-v2": "^1.9.0",
    "ts-md5": "^1.3.1",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@types/crypto-js": "^4.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.12.2",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.24.0",
    "husky": "^8.0.3",
    "jsencrypt": "^3.3.2",
    "postcss": "8.4.38",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.72.0",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.6.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^5.3.2",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.25.2",
    "vite": "6.3.5",
    "vite-plugin-compression2": "^0.10.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.2.10"
  }
}
