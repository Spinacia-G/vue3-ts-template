{
  "name": "template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 3456",
    "build": "vue-tsc -b && vite build --mode production",
    "build-only": "vite build --mode production",
    "preview": "vite preview --host --open",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx,.vue src --fix",
    "lint:style": "stylelint \"src/**/*.{vue,css}\"",
    "lint:style-fix": "stylelint \"src/**/*.{vue,css}\" --fix",
    "format": "prettier --write \"**/*.{vue,html,css,js,ts,jsx,tsx,json,md}\"",
    "prepare": "husky",
    "font-subset": "python plugins/font-subset.py",
    "zip": "python plugins/zip.py --prefix template"
  },
  "dependencies": {
    "@vueuse/core": "13.9.0",
    "dayjs": "^1.11.18",
    "element-plus": "~2.11.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.0.0",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.33.0",
    "@tailwindcss/vite": "^4.1.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "stylelint": "^16.23.1",
    "stylelint-config-recess-order": "^7.2.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.40.0",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-element-plus": "^0.10.0",
    "unplugin-vue-components": "^29.0.0",
    "vite": "^7.1.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.0.5"
  },
  "lint-staged": {
    "**/*.{vue,html,css,js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "**/*.{js,ts,jsx,tsx,vue}": [
      "eslint --fix"
    ],
    "src/**/*.{css,vue}": [
      "stylelint --fix"
    ]
  }
}
